{
  "name": "PowerPlay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "debugAssembleAndroid": "ENVFILE=.env npx react-native run-android",
    "assembleDevAndroid": "cd android && ./gradlew clean && ENVFILE=.env ./gradlew assembleRelease",
    "assembleProdAndroid": "cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew assembleRelease",
    "bundleProdAndroid": "cd android && ./gradlew clean && ENVFILE=.env.production ./gradlew bundleRelease",
    "debugAndroidWindows": "SET ENVFILE=.env && npx react-native run-android",
    "assembleAndroidWindows": "cd android && gradlew clean && SET ENVFILE=.env && gradlew assembleRelease",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.11.0",
    "react": "18.2.0",
    "react-native": "0.74.7",
    "react-native-config": "^1.5.9",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "4.5.0",
    "react-native-vector-icons": "^10.3.0",
    "reactotron-react-native": "^5.1.15"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.89",
    "@react-native/eslint-config": "0.74.89",
    "@react-native/metro-config": "0.74.89",
    "@react-native/typescript-config": "0.74.89",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-make": "^1.0.1",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
